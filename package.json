  "repository": {
    "type": "git",
    "url": "https://github.com/atomist/slalom.git"
  },
  "main": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "atomist",
    "automation",
    "github"
  ],
  "homepage": "https://github.com/atomist/slalom#readme",
  "bugs": {
    "url": "https://github.com/atomist/slalom/issues"
  },
  "dependencies": {
    "@atomist/slack-messages": "^0.12.1",
    "@octokit/rest": "^15.9.5",
    "@types/body-parser": "^1.17.0",
    "@types/inquirer": "0.0.42",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/jssha": "0.0.29",
    "@types/marked": "^0.4.0",
    "@types/node-notifier": "0.0.28",
    "@types/sprintf-js": "^1.1.0",
    "app-root-path": "^2.0.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "boxen": "^1.3.0",
    "chalk": "^2.4.1",
    "express": "^4.16.3",
    "find-process": "^1.1.1",
    "fs-extra": "^6.0.1",
    "glob-promise": "^3.4.0",
    "inquirer": "^6.0.0",
    "json-stringify-safe": "^5.0.1",
    "jssha": "^2.3.1",
    "lodash": "^4.17.10",
    "marked": "^0.4.0",
    "marked-terminal": "^3.0.0",
    "node-notifier": "^5.2.1",
    "open": "0.0.5",
    "portfinder": "^1.0.13",
    "sprintf-js": "^1.1.1",
    "yargs": "^12.0.1"
  },
  "peerDependencies": {
    "@atomist/automation-client": "*",
    "@atomist/sdm": "*",
    "@atomist/sdm-core": "*"
  },
  "devDependencies": {
    "@atomist/automation-client": "^0.19.5",
    "@atomist/sdm": "^0.4.0",
    "@atomist/sdm-core": "^0.4.0",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.10.17",
    "@types/power-assert": "^1.4.29",
    "@types/yargs": "^11.1.1",
    "axios-mock-adapter": "^1.15.0",
    "espower-typescript": "^8.1.3",
    "mocha": "^4.1.0",
    "npm-run-all": "^4.1.3",
    "power-assert": "^1.4.4",
    "prettier": "^1.13.0",
    "rimraf": "^2.6.2",
    "supervisor": "^0.12.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.11.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3",
    "typescript-formatter": "^7.2.0"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "autotest": "supervisor --watch index.ts,src,test --extensions ts --no-restart-on exit --quiet --exec npm -- test",
    "build": "npm-run-all compile test lint doc",
    "clean": "npm-run-all clean:js clean:build",
    "clean:build": "rimraf *-v8.log profile.txt coverage typedoc",
    "clean:dist": "npm-run-all clean clean:npm",
    "clean:js": "rimraf \"{src,test}/**/*.{js,d.ts}\" \"*.{js,d.ts}\"    ",
    "clean:npm": "rimraf node_modules",
    "compile": "npm-run-all git:info compile:ts",
    "compile:ts": "tsc --project .",
    "debug": "node $NODE_DEBUG_OPTION node_modules/@atomist/automation-client/start.client.js",
    "doc": "typedoc --mode modules --ignoreCompilerErrors --exclude \"**/*.d.ts\" --out doc index.ts src",
    "fmt": "npm-run-all fmt:ts",
    "fmt:ts": "tsfmt --replace",
    "git:info": "atomist git",
    "lint": "npm-run-all lint:ts",
    "lint:ts": "tslint --format verbose --project .",
    "lint:fix": "npm-run-all lint:ts:fix",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "test": "mocha --exit --require ts-node/register --require source-map-support/register \"test/**/*est.ts\"",
    "typedoc": "npm run doc"
  },
  "engines": {
    "node": ">=9.2.0",
    "npm": ">=5.0.0"
  }
}
